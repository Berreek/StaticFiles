    "emitEntryPoint": true
  },
  "dependencies": {
    "Microsoft.AspNetCore.Server.IISIntegration": "1.0.5",
    "Microsoft.AspNetCore.Server.Kestrel": "1.0.6",
    "Microsoft.AspNetCore.StaticFiles": "1.0.5",
    "Microsoft.Extensions.Logging.Console": "1.0.2"
  },
  "frameworks": {
    "net451": {},
