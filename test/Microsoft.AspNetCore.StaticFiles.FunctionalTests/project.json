{
  "buildOptions": {
    "warningsAsErrors": true,
    "keyFile": "../../tools/Key.snk",
    "copyToOutput": {
      "include": [
        "SubFolder/**/*",
        "TestDocument.txt",
        "TestDocument1MB.txt"
      ]
    }
  },
  "publishOptions": {
    "include": [
      "SubFolder/**/*",
      "TestDocument.txt"
    ]
  },
  "dependencies": {
    "dotnet-test-xunit": "2.2.0-*",
    "Microsoft.AspNetCore.Server.IntegrationTesting": "0.2.3",
    "Microsoft.AspNetCore.Server.Kestrel": "1.1.3",
    "Microsoft.AspNetCore.Server.WebListener": "1.1.4",
    "Microsoft.AspNetCore.StaticFiles": "1.1.3",
    "Microsoft.AspNetCore.Testing": "1.1.1",
    "Microsoft.Extensions.Logging.Testing": "1.1.2",
    "xunit": "2.2.0-*"
  },
  "frameworks": {
    "netcoreapp1.1": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.1.7",
          "type": "platform"
        }
      }
    },
    "net451": {
      "frameworkAssemblies": {
        "System.Net.Http": ""
      }
    }
  },
  "testRunner": "xunit"
}